<?xml version="1.0" encoding="UTF-8"?> <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mapper.MainMapper">
	<select id="selectMember" resultType="int" parameterType="int">
		SELECT COUNT(id) FROM member
	</select>
	<select id="selectMyData" resultType="com.vo.member" parameterType="int">
		SELECT nm, userId, birth, pno, addr1, addr2, bodySize, lv FROM YULSHOP.member WHERE id = #{value}
	</select>
	
</mapper>
